{
  "dry-run": true,
  "dim": 2,
  "partitionLevel": 6,
  "uniformLevel": 6,
  "maxRfnmtLvl": 6,
  "maxNoCells": 1000000,
  "outputDir": "out",
  "gridFileName": "gridD",
  "gridOutput": {
    "format": "VTKB",
    "cellFilter": "leafCells",
    "type": "points",
    "outputValues": [
      "level",
      "noChildren"
    ]
  },
  "geometry": {
    "cube": {
      "type": "box",
      "body": "flowregion",
      "A": [
        0.0,
        0.0
      ],
      "B": [
        10.0,
        7.0
      ]
    },
    "step_a": {
      "type": "box",
      "body": "flowregion",
      "subtract": false,
      "A": [
        0.0,
        0.0
      ],
      "B": [
        2.0,
        10.0
      ]
    },
    "step_b": {
      "type": "box",
      "body": "flowregion",
      "subtract": false,
      "A": [
        4.0,
        0.0
      ],
      "B": [
        6.0,
        10.0
      ]
    },
    "step_c": {
      "type": "box",
      "body": "flowregion",
      "subtract": false,
      "A": [
        8.0,
        0.0
      ],
      "B": [
        10.0,
        10.0
      ]
    }
  },
  "solver": {
    "type": "lbm",
    "equation": "poisson",
    "model": "D2Q9",
    "relaxation": 1.0,
    "maxSteps": 10000,
    "info_interval": 10,
    "solution_interval": 1000,
    "convergence": 2E-14,
    "errorL2": 5E-3,
    "errorMax": 1.1E-3,
    "assumeAxisAligned": true,
    "boundary": {
      "box": {
        "+x": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1
        },
        "-x": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1
        },
        "+y": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1
        },
        "-y": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1
        }
      }
    },
    "postprocessing": {
      "line1": {
        "type": "line",
        "execute": "atEnd",
        "A": [
          0.501,
          0.0
        ],
        "B": [
          0.501,
          1.0
        ]
      }
    }
  }
}