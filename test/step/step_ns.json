      "step_a": {
        "+x": {
          "type": "wall",
          "model": "bounceback"
        },
        "-x": {
          "type": "pressure",
          "pressure": 1.0000008
        },
        "+y": {
          "type": "wall",
          "model": "bounceback"
        },
        "-y": {
          "type": "wall",
          "model": "bounceback"
        }
      },
      "step_b": {
        "+x": {
          "type": "pressure",
          "pressure": 1.0
        },
        "-x": {
          "type": "wall",
          "model": "bounceback"
        },
        "+y": {
          "type": "wall",
          "model": "bounceback"
        },
        "-y": {
          "type": "wall",
          "model": "bounceback"
        }
      }
