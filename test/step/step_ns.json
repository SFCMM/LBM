{
  "dry-run": true,
  "dim": 2,
  "partitionLevel": 6,
  "uniformLevel": 6,
  "maxRfnmtLvl": 6,
  "maxNoCells": 1000000,
  "outputDir": "out",
  "gridFileName": "gridD",
  "gridOutput": {
    "format": "VTKB",
    "cellFilter": "leafCells",
    "type": "points",
    "outputValues": [
      "level",
      "noChildren"
    ]
  },
  "geometry": {
    "cube": {
      "type": "box",
      "body": "flowregion",
      "A": [
        0.0,
        0.0
      ],
      "B": [
        10.0,
        9.0
      ]
    },
    "step_a": {
      "type": "box",
      "body": "flowregion",
      "subtract": false,
      "A": [
        0.0,
        9.0
      ],
      "B": [
        4.0,
        10.0
      ]
    },
    "step_b": {
      "type": "box",
      "body": "flowregion",
      "subtract": false,
      "A": [
        6.0,
        9.0
      ],
      "B": [
        10.0,
        10.0
      ]
    }
  },
  "solver": {
    "type": "lbm",
    "reynoldsnumber": 0.1,
    "ma": 0.0001,
    "poiseuillePressureGradient": 0.0000008,
    "analyticalSolutionExcludeSurface": [
