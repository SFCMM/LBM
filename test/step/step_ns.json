    "maxSteps": 100000,
    "info_interval": 100,
    "solution_interval": 1000,
    "convergence": 2E-11,
    "errorL2": 3.3E-5,
    "errorMax": 1.3E-7,
    "boundary": {
      "cube": {
        "+x": {
