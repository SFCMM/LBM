          "pressure": 1.0
        },
        "-x": {
          "type": "pressure",
          "pressure": 1.0000008
        },
        "-y": {
          "type": "wall",
          "model": "bounceback"
        },
        "+y": {
          "type": "wall",
          "model": "bounceback"
        }
