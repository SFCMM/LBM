{
  "dry-run": true,
  "dim": 2,
  "partitionLevel": 5,
  "uniformLevel": 5,
  "maxRfnmtLvl": 5,
  "maxNoCells": 1000000,
  "outputDir": "out",
  "gridFileName": "gridD",
  "gridOutput": {
    "format": "VTKB",
    "cellFilter": "leafCells",
    "type": "points",
    "outputValues": [
      "level",
      "noChildren"
    ]
  },
  "geometry": {
    "cube": {
      "type": "box",
      "A": [
        0.0,
        0.0
      ],
      "B":[
        3.0,
        5.0
      ]
    }
  },
  "solver": "lbm",
  "boundary": {
    "cube": {
      "+x":{
        "type": "periodic",
        "connection": "-x"
      },
      "-x":{
        "type": "periodic",
        "connection": "+x"
      },
      "-y": {
        "type": "wall",
        "model": "bounceback",
        "tangential velocity": 1.0
      },
      "+y": {
        "type": "wall",
        "model": "bounceback"
      }
    }
  }
}