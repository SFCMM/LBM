{
  "dry-run": true,
  "dim": 2,
  "partitionLevel": 5,
  "uniformLevel": 5,
  "maxRfnmtLvl": 5,
  "maxNoCells": 1000000,
  "outputDir": "out",
  "gridFileName": "gridD",
  "gridOutput": {
    "format": "VTKB",
    "cellFilter": "leafCells",
    "type": "points",
    "outputValues": [
      "level",
      "noChildren"
    ]
  },
  "geometry": {
    "cube": {
      "type": "box",
      "A": [
        0.0,
        0.0
      ],
      "B": [
        1.0,
        1.0
      ]
    }
  },
  "solver": {
    "type": "lbm",
    "relaxation": 1,
    "reynoldsnumber": 1.25,
    "analyticalSolution": "poiseuille2D_1",
    "analyticalSolutionExcludeSurface": [
      "+x",
      "-x"
    ],
    "maxSteps": 100000,
    "info_interval": 100,
    "solution_interval": 10000,
    "convergence": 2E-11,
    "errorL2": 1.1E-4,
    "errorMax": 6E-4,
    "assumeAxisAligned": true,
    "boundary": {
      "cube": {
        "+x": {
          "type": "periodic",
          "generateBndry": true,
          "connection": "-x",
          "pressure": 1.00001
        },
        "-x": {
          "type": "periodic",
          "generateBndry": true,
          "connection": "+x",
          "pressure": 1.0
        },
        "-y": {
          "type": "wall",
          "model": "bounceback"
        },
        "+y": {
          "type": "wall",
          "model": "bounceback"
        }
      }
    },
    "postprocessing": {
      "line1": {
        "type": "line",
        "execute": "atEnd",
        "A": [
          0.501,
          0.0
        ],
        "B": [
          0.501,
          1.0
        ]
      }
    }
  }
}