{
  "dry-run": true,
  "dim": 2,
  "partitionLevel": 5,
  "uniformLevel": 5,
  "maxRfnmtLvl": 5,
  "maxNoCells": 1000000,
  "outputDir": "out",
  "gridFileName": "gridD",
  "alignNodesWithSurface": true,
  "gridOutput": {
    "format": "VTKB",
    "cellFilter": "leafCells",
    "type": "points",
    "outputValues": [
      "level",
      "noChildren"
    ]
  },
  "geometry": {
    "cube": {
      "type": "box",
      "A": [
        0.0,
        0.0
      ],
      "B": [
        1.0,
        1.0
      ]
    }
  },
  "solver": {
    "type": "lbm",
    "relaxation": 1,
    "ma": 0.0001,
    "analyticalSolution": "poiseuille2D_1",
    "poiseuillePressureGradient": 0.000133328,
    "analyticalSolutionExcludeSurface": [
      "+x",
      "-x"
    ],
    "maxSteps": 75000,
    "info_interval": 100,
    "solution_interval": 10000,
    "convergence": 1E-14,
    "errorL2": 3.3E-5,
    "errorMax": 1.3E-7,
    "assumeAxisAligned": true,
    "boundary": {
      "cube": {
        "+x": {
          "type": "periodic",
          "generateBndry": true,
          "connection": "-x",
          "pressure": 1.000133328
        },
        "-x": {
          "type": "periodic",
          "generateBndry": true,
          "connection": "+x",
          "pressure": 1.0
        },
        "-y": {
          "type": "wall",
          "model": "equilibrium"
        },
        "+y": {
          "type": "wall",
          "model": "equilibrium"
        }
      }
    },
    "postprocessing": {
      "line1": {
        "type": "line",
        "execute": "atEnd",
        "A": [
          0.501,
          0.0
        ],
        "B": [
          0.501,
          1.0
        ]
      }
    }
  }
}