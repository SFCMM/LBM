{
  "dry-run": true,
  "dim": 2,
  "solver": {
    "type": "lpt",
    "method": "bgk",
    "relaxation": 0.9,
    "reynoldsnumber": 0.75,
    "maxSteps": 200000,
    "info_interval": 100,
    "solution_interval": 500,
    "output_dir": "out",
    "solution_filename": "couette",
    "convergence": 2E-11,
    "analyticalSolution": "couette2D_1_5",
    "errorL2": 7E-10,
    "errorMax": 4E-11,
    "assumeAxisAligned": true,
    "boundary": {
      "cube": {
        "+x": {
          "type": "periodic",
          "generateBndry": false,
          "connection": "-x"
        },
        "-x": {
          "type": "periodic",
          "generateBndry": false,
          "connection": "+x"
        },
        "+y": {
          "type": "wall",
          "model": "bounceback",
          "tangentialVelocity": 0.1
        },
        "-y": {
          "type": "wall",
          "model": "bounceback"
        }
      }
    }
  }
}