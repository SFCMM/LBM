{
  "dry-run": true,
  "dim": 2,
  "partitionLevel": 7,
  "uniformLevel": 7,
  "maxRfnmtLvl": 7,
  "maxNoCells": 1000000,
  "outputDir": "out",
  "gridFileName": "gridD",
  "output": {
    "format": "VTKB",
    "cellFilter": "leafCells",
    "type": "points",
    "outputValues": [
      "level",
      "noChildren"
    ]
  },
  "geometry": {
    "line": {
      "type": "box",
      "A": [
        0.0,
        0.0
      ],
      "B": [
        1.0,
        1.0
      ]
    }
  },
  "solver": {
    "type": "lbm",
    "equation": "poisson",
    "model": "D2Q9",
    "relaxation": 1.0,
    "maxSteps": 10000,
    "info_interval": 1,
    "solution_interval": 10,
    "convergence": 2E-14,
    "errorL2": 5E-3,
    "errorMax": 1.1E-3,
    "assumeAxisAligned": true,
    "boundary": {
      "line": {
        "+x": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1
        },
        "-x": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1
        },
        "+y": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1
        },
        "-y": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1
        }
      }
    },
    "postprocessing": {
      "line1": {
        "type": "line",
        "execute": "atEnd",
        "A": [
          0.501,
          0.0
        ],
        "B": [
          0.501,
          2.0
        ]
      }
    }
  }
}