{
  "dry-run": true,
  "dim": 1,
  "partitionLevel": 5,
  "uniformLevel": 5,
  "maxRfnmtLvl": 5,
  "maxNoCells": 1000000,
  "outputDir": "out",
  "gridFileName": "gridD",
  "gridOutput": {
    "format": "VTKB",
    "cellFilter": "leafCells",
    "type": "points",
    "outputValues": [
      "level",
      "noChildren"
    ]
  },
  "geometry": {
    "line": {
      "type": "box",
      "A": [
        0.0
      ],
      "B": [
        1.0
      ]
    }
  },
  "solver": {
    "type": "lbm",
    "equation": "poisson",
    "model": "D1Q3",
    "relaxation": 1.0,
    "maxSteps": 100,
    "info_interval": 1,
    "solution_interval": 1,
    "convergence": 1E-14,
    "errorL2": 5E-3,
    "errorMax": 1.1E-3,
    "assumeAxisAligned": true,
    "boundary": {
      "line": {
        "+x": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1.0
        },
        "-x": {
          "type": "dirichlet",
          "model": "neem",
          "value": 1.0
        }
      }
    }
  }
}